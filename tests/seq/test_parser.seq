# Test parser

include "../../src/parser"

: main ( -- )
  "Testing parser:" io.write-line

  # Simple number
  "42" print-parse

  # Simple symbol
  "hello" print-parse

  # Simple list
  "(+ 1 2)" print-parse

  # Nested list
  "(define x (+ 1 2))" print-parse

  # More complex
  "(if (> x 0) 1 -1)" print-parse

  # Quote syntax
  "'x" print-parse
  "'(1 2 3)" print-parse
;
